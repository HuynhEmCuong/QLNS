					STT.setAttribute('class','stt');
					STT.setAttribute('onclick',"addClick("+i+",'"+dataSearch[i]+"')");
					if((i+1)==ResNum){
						STT.setAttribute('class','stt bo-bottom');
					}
					STT.innerHTML = (i+1);
					
					//FileName
					var FileName = document.createElement('div');
					FileName.setAttribute('class','mssv text-left');
					FileName.setAttribute('onclick',"addClick("+i+",'"+dataSearch[i]+"')");
					if((i+1)==ResNum){
						FileName.setAttribute('class','mssv bo-bottom text-left');
					}
					FileName.setAttribute('style','padding-left:2px');
					//FileName.innerHTML = dataSearch.Result[i].FileName;
					
					var link = document.createElement('a');
					link.setAttribute('id',"a"+i);
					link.setAttribute('target',"_blank");
					link.setAttribute('href','../../Files/uploads/'+dataSearch.Result[i].StaffID+'/'+dataSearch.Result[i].FileName);
					link.setAttribute('style','text-decoration: underline;color: blue;');
					link.innerHTML =  dataSearch.Result[i].FileName;
					FileName.appendChild(link);

					//Description
					var Description = document.createElement('div');
					Description.setAttribute('class','name text-left');
					Description.setAttribute('style','width:600px;');
					Description.setAttribute('contenteditable','true');
					Description.setAttribute('onclick',"addClick("+i+",'"+dataSearch[i]+"')");
					if((i+1)==ResNum){
						Description.setAttribute('class','name text-left bo-bottom');
					}
					Description.innerHTML = dataSearch.Result[i].Description;
					
					//CreateDate
					var CreateDate = document.createElement('div');
					CreateDate.setAttribute('class','name');
					CreateDate.setAttribute('onclick',"addClick("+i+",'"+dataSearch[i]+"')");
					if((i+1)==ResNum){
						CreateDate.setAttribute('class','name bo-bottom');
					}
					CreateDate.innerHTML = dataSearch.Result[i].CreateDate;
					
					//LastUpdate
					var LastUpdate = document.createElement('div');
					LastUpdate.setAttribute('class','name');
					LastUpdate.setAttribute('style','border-right:none');
					LastUpdate.setAttribute('onclick',"addClick("+i+",'"+dataSearch[i]+"')");
					if((i+1)==ResNum){
						LastUpdate.setAttribute('class','name bo-bottom');
					}
					LastUpdate.innerHTML = dataSearch.Result[i].LastUpdate;
					
					var deleteImg = document.createElement('div');
					deleteImg.setAttribute('id','del-'+i);
					deleteImg.setAttribute('class','iconDelete name');
					if((i+1)==ResNum){
						deleteImg.setAttribute('class','iconDelete name bo-bottom');
					}
					deleteImg.setAttribute('onClick',"deleteRow('"+i+"')");
					
					//check button
					var checkbtn = document.createElement('input');
					checkbtn.setAttribute('id','check-'+i);
					checkbtn.setAttribute('hidden','');
					checkbtn.value=0;
					
				row.appendChild(STT);
				row.appendChild(FileName);
				row.appendChild(Description);
				row.appendChild(CreateDate);
				row.appendChild(LastUpdate);
				row.appendChild(checkbtn);
				row.appendChild(deleteImg);
				
				content.appendChild(row);
			}
		}
		
