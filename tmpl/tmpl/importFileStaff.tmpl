	<script type="text/javascript">
		function loadListStaff(){
			var url = "ajaxLoadListStaff.htm";
			queryString = "status='0'";
			queryString += '&permisstion='+'{val permission}';
			
			htmlRequest = ajaxFunction();
			if (htmlRequest==null){ // If it cannot create a new Xmlhttp object.
				alert ("Browser does not support HTTP Request");
				return;
			}
			htmlRequest.onreadystatechange = function(){
				if(htmlRequest.readyState == 4){
				//	alert(htmlRequest.responseText);
					dat = htmlRequest.responseText;
				
					dat = eval('(' + dat + ')');
					
					loadStaff(dat);
				}
			}
			
			htmlRequest.open("POST", url,true);
			htmlRequest.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			htmlRequest.send(queryString);
		}
		
		function loadStaff(dat){
			stt = 1;
			var staffObj = document.getElementById("staff");
			staffObj.innerHTML = "";
			
			var option = document.createElement('option');
			option.innerHTML = "--Choose Staff --";
			option.value=0;
			staffObj.appendChild(option);
			
			if(dat!=''){
				depNameList=dat.Result;
				
				for(var i=0; i< depNameList.length; i++){
					var option = document.createElement('option');
					option.innerHTML =  depNameList[i].StaffID+'-'+depNameList[i].FullName;
					option.value =  depNameList[i].StaffID;
					staffObj.appendChild(option);
				}
			}
		}
		
		function enableCheckIcon(){
			document.getElementById("mssv").value="";
			removeClass(document.getElementById('iconArrow'),'display-none');
			addClass(document.getElementById('iconRed'),'display-none');
			addClass(document.getElementById('iconBlue'),'display-none');
		}
		
		function showPopUp(){
			addClass(document.getElementById('mssv'),'display-none');
			openPopup("unlockApp","password");
		}
		
		function changeOption(f){
			document.getElementById('staffID').value = f.staff.options[f.staff.selectedIndex].value;
		}
		
		function submitInfo()
		{
			CreateForm.submit();
		}
		
		$(document).ready(function(){
			$('.multi').MultiFile({
				accept:'any', max:5, STRING: {
				remove:'Remover',
				selected:'Selecionado: $file',
				denied:'Invalido arquivo de tipo $ext!',
				duplicate:'Arquivo ja selecionado:\n$file!'
			}
			});
		
			/*$('#submit').click(function() {
