		function checkDate(obj,msg,numberMin,numberMax){
			var msg = msg+' is invalid';
			if(isNaN(obj.value)==true){
				alert(msg);
				obj.value = '';
				document.getElementById(obj.id).focus();
				return;
			}else{
				if(obj.value.length<numberMin||obj.value.length>numberMax){
					alert(msg);
					obj.value = '';
					document.getElementById(obj.id).focus();
					return;
				}
			}
			delete(msg);
			return true;
		}
		
