					strData+='&manu='+document.getElementById("txtManuCode").value;
				}
				if(document.getElementById("txtPO").value!="PO Number")
				{
					strData+='&PO='+document.getElementById("txtPO").value;
				}
				if(document.getElementById("txtAmount").value!="Amount")
				{
