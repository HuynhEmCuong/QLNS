	<script type="text/javascript">
		function process()
		{
			var curDate = new Date(); 
			var year = curDate.getFullYear(); 
			var month = curDate.getMonth() + 1; 
			var date = curDate.getDate(); 
			if (month < 10) month = "0" + month; 
			if (date < 10) date = "0" + date; 
		
			document.getElementById('month').innerHTML = month;
			document.getElementById('year').innerHTML = year;
			document.getElementById('date').innerHTML = date;
			
			document.getElementById('month1').innerHTML = month;
			document.getElementById('year1').innerHTML = year;
			document.getElementById('date1').innerHTML = date;
			
			document.getElementById('month2').innerHTML = month;
			document.getElementById('year2').innerHTML = year;
			document.getElementById('date2').innerHTML = date;
			
			if('{val NguyenVong}'==2 || '{val NguyenVong}'==3){
				addClass(document.getElementById('content1'),'display-none');
			}else{
				removeClass(document.getElementById('content1'),'display-none');
			}
		}

		//hàm dùng d? set d? li?u cho các input
		function save(type){
			if('{val StudentID}' != '' && '{val Address}'!='' && '{val district}' != 0){
				saveInfo = "{";
				saveInfo += '"IRN":"'+'{val IRN}'+'",';
				saveInfo += '"Birthday":"'+'{val Birthday}'+'",';
				saveInfo += '"RegisNum":"'+'{val RegisNum}'+'",';
				saveInfo += '"FullName":"'+'{val FullName}'+'",';
				saveInfo += '"StudentID":"'+'{val StudentID}'+'",';
				saveInfo += '"Phone":"'+'{val Phone}'+'",';
				saveInfo += '"Mobile":"'+'{val Mobile}'+'",';
				saveInfo += '"PrivateEmail":"'+'{val PrivateEmail}'+'",';
				saveInfo += '"SchoolEmail":"'+'{val SchoolEmail}'+'",';
				saveInfo += '"RoomID":"'+'{val RoomID}'+'",';
				saveInfo += '"district":"'+'{val district}'+'",';
				saveInfo += '"districtName":"'+'{val districtName}'+'",';
				saveInfo += '"Address":"'+'{val Address}'+'"';
				saveInfo += "}";
			//	alert(saveInfo);
			//	saveInfo = saveInfo.replace(/\"/g, "%22");
			//	alert(document.getElementById('saveBut'));
			//	document.getElementById("jsonString").value = saveInfo;
			//	PrintPreview.submit();
			
				addClass(document.getElementById("btn-1"),'display-none');
				addClass(document.getElementById("btn-2"),'display-none');
				//addClass(document.getElementById("editBut"),'display-none');
				if(type==1){
					saveInfo = saveInfo.replace(/&/g, "%26");
					saveInfo = saveInfo.replace(/\+/g, "%2B");
					saveInfo = saveInfo.replace(/\'/g, "''");
					updateAjax(saveInfo);
				}else{
					document.getElementById("jsonString").value = saveInfo;
					DetailInfo.submit();
				}							
				
			}else{
				if(type==1){
					alert('CMND, địa chỉ liên hệ và tỉnh/thành phố không được phép rỗng.');
				}				
			}
			
		}
		
	
		
		function updateAjax(saveInfo) {
		//	alert(saveInfo);exit();
			document.getElementById("saveBut").disabled = true;
			removeClass(document.getElementById('iconLogin'),'display-none');
			var url = "ajaxInsertInfo.htm";
