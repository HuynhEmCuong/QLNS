		//	alert(queryString);
			htmlRequest = ajaxFunction();
			if (htmlRequest==null){ // If it cannot create a new Xmlhttp object.
				alert ("Browser does not support HTTP Request");
				return;
			}
			
			htmlRequest.onreadystatechange = function(){
				if(htmlRequest.readyState == 4){
				//	alert(htmlRequest.responseText);exit();
					
					if(trim(htmlRequest.responseText)=='EXIST'){
						addClass(document.getElementById('iconLogin'),'display-none');
					//	alert("Thông tin sinh viên đã được nhập!");
						document.getElementById("saveBut").disabled = false;
						
					//	alert(document.getElementById('titleClick').className);
						document.getElementById('titleClick').setAttribute("onClick","back('mainpage')");
						document.getElementById('titleBack').setAttribute("onClick","back('mainpage')");
						addClass(document.getElementById('titleClick'),"cursor");
						addClass(document.getElementById('titleBack'),"cursor");
						
						//In ra bien nhan
						//back("mainpage");
						window.print();
					//	back("mainpage");
					}else if(trim(htmlRequest.responseText)=='NG'){
						addClass(document.getElementById('iconLogin'),'display-none');
						alert("Không thể in được. Vui lòng kiểm tra lại thông tin!");
						document.getElementById("saveBut").disabled = false;
					}else{
					//	alert("Thêm thông tin sinh viên thành công.");
						addClass(document.getElementById('iconLogin'),'display-none');
					//	document.getElementById("saveBut").disabled = false;
					//	document.getElementById("editBut").disabled = true;
					
						var dataSearch = trim(htmlRequest.responseText);
						dataSearch = eval('(' + dataSearch + ')');

						document.getElementById("idHoSo").innerHTML = dataSearch.stt;
						document.getElementById("email").innerHTML = dataSearch.email;
						
						document.getElementById("idHoSo2").innerHTML = dataSearch.stt;
						document.getElementById("email2").innerHTML = dataSearch.email;
						
						if('{val NguyenVong}' == 1){
							document.getElementById("time").innerHTML = dataSearch.time;
							document.getElementById("dateRoom").innerHTML = dataSearch.date;
							document.getElementById("location").innerHTML = dataSearch.location;
							if(dataSearch.time=='' || dataSearch.date=='' || dataSearch.location== ''){
								alert("Đã hết phòng trống.");
							}						
						}
						//	alert(document.getElementById('titleClick').className);
							document.getElementById('titleClick').setAttribute("onClick","back('mainpage')");
							document.getElementById('titleBack').setAttribute("onClick","back('mainpage')");
							addClass(document.getElementById('titleClick'),"cursor");
							addClass(document.getElementById('titleBack'),"cursor");
							
						//In ra bien nhan
						//back("mainpage");
						window.print();
						//back("mainpage");
					}
					
				/*	if(trim(htmlRequest.responseText)=='OK'){
